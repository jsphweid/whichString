{
    "compilerOptions": {
        "outDir": "build/",
        "module": "esnext",
        "target": "es5",
        "skipLibCheck": true,
        "moduleResolution": "node",
        "forceConsistentCasingInFileNames": true,
        "noImplicitReturns": true,
        "noImplicitThis": true,
        "noImplicitAny": true,
        "strictNullChecks": true,
        "suppressImplicitAnyIndexErrors": true
    },
    "files": [
        "src/server/server.ts"
    ]
}
